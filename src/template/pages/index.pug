extends ../_layouts/master.pug
block title
	+title("Redsoft UI Kit")
block content
	.container
		.b-typographie
			h3 Заголовки
				h1 Redsoft
				h2 Redsoft
				h3 Redsoft
				h4 Redsoft
				h5 Redsoft
				h6 Redsoft
		.b-buttons.b-component
			h3 Кнопки
			a(href="javascript:void(0)", class="btn btn-primary") btn btn-primary
			a(href="javascript:void(0)", class="btn btn-border") btn btn-border
			a(href="javascript:void(0)", class="btn btn-fill") btn btn-fill
		.b-forms.b-component
			h3 Формы
			+rsForm()
		.b-carousel.b-component
			h3 Слайдеры
			+carousel()